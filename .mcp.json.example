{
  "mcpServers": {
    "ching-tech-os": {
      "type": "stdio",
      "command": "bash",
      "args": ["-c", "cd /path/to/ching-tech-os/backend && uv run python -m ching_tech_os.mcp_cli"]
    },
    "erpnext": {
      "type": "stdio",
      "command": "bash",
      "args": [
        "-c",
        "set -a && source /path/to/ching-tech-os/.env && set +a && uvx erpnext-mcp"
      ]
    },
    "nanobanana": {
      "type": "stdio",
      "command": "bash",
      "args": [
        "-c",
        "set -a && source /path/to/ching-tech-os/.env && set +a && uvx nanobanana-py"
      ]
    },
    "printer": {
      "type": "stdio",
      "command": "uvx",
      "args": ["printer-mcp"]
    }
  }
}
